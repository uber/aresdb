          "$ref": "#/definitions/Duration"
        },
        "foreignTableCommonFilters": {
          "description": "ForeignTableCommonFilters match filters with foreign table columns involved",
          "type": "array",
          "items": {
            "$ref": "#/definitions/Expr"
          },
          "x-go-name": "ForeignTableCommonFilters"
        },
        "liveStats": {
          "$ref": "#/definitions/oopkQueryStats"
        },
        "mainTableCommonFilters": {
          "description": "Filters that apply to all archive and live batches.\nMainTableCommonFilters match filters with only main table columns involved",
          "type": "array",
          "items": {
            "$ref": "#/definitions/Expr"
          },
